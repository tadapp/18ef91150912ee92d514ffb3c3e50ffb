<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = ".tw1.ru.co16812-wordpress-6"
        versionCode = "10"
        version     = "1.0.0">

    <name>Tadapp.DEMO</name>

    <description>
        wdwddwdwdwdwdwdwdwddwdd
    </description>

    <author>Tadapp</author>

    <plugin name='cordova-plugin-splashscreen' />
    <plugin name='cordova-plugin-inappbrowser' />
    <plugin name='cordova-plugin-network-information' />

    <preference name='phonegap-version' value='cli-9.0.0' />

    <gap:plugin name='onesignal-cordova-plugin' spec='2.4.5' source='npm' />

    <preference name='android-build-tool' value='gradle' />
    <preference name="android-targetSdkVersion" value="28" />
    <preference name='orientation' value='portrait' />

    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="SplashScreenSpinnerColor" value="#00033f" />

    <preference name="Fullscreen" value="false" />


    <platform name="android">
        <icon src="res/android/icon/ldpi_1576163649_451150662095f606fd39.png" density="ldpi" />
        <icon src="res/android/icon/mdpi_1576163649_451150662095f606fd39.png" density="mdpi" />
        <icon src="res/android/icon/hdpi_1576163649_451150662095f606fd39.png" density="hdpi" />
        <icon src="res/android/icon/xhdpi_1576163649_451150662095f606fd39.png" density="xhdpi" />
        <icon src="res/android/icon/xxhdpi_1576163649_451150662095f606fd39.png" density="xxhdpi" />
        <icon src="res/android/icon/xxxhdpi_1576163649_451150662095f606fd39.png" density="xxxhdpi" />

        <splash src="res/android/screen/hdpi_port_1576163661_2e2490fb17394bc67cab.png" density="port-hdpi"/>
        <splash src="res/android/screen/ldpi_port_1576163661_2e2490fb17394bc67cab.png" density="port-ldpi"/>
        <splash src="res/android/screen/mdpi_port_1576163661_2e2490fb17394bc67cab.png" density="port-mdpi"/>
        <splash src="res/android/screen/xhdpi_port_1576163661_2e2490fb17394bc67cab.png" density="port-xhdpi"/>
        <splash src="res/android/screen/hdpi_land_1576163661_2e2490fb17394bc67cab.png" density="land-hdpi"/>
        <splash src="res/android/screen/ldpi_land_1576163661_2e2490fb17394bc67cab.png" density="land-ldpi"/>
        <splash src="res/android/screen/mdpi_land_1576163661_2e2490fb17394bc67cab.png" density="land-mdpi"/>
        <splash src="res/android/screen/xhdpi_land_1576163661_2e2490fb17394bc67cab.png" density="land-xhdpi"/>
    </platform>

    <access origin='*'/>
    <plugin name='cordova-plugin-whitelist' />
    <allow-intent href='http://*/*'/>
    <allow-intent href='https://*/*'/>
    <allow-intent href='tel:*'/>
    <allow-intent href='sms:*'/>
    <allow-intent href='mailto:*'/>
    <allow-intent href='geo:*'/>

</widget>